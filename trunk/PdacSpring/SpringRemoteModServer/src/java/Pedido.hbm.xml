        <id name="o_orderkey" column="o_orderkey" />
        <property name="o_custkey" column="o_custkey" />
        <property name="o_totalprice" column="o_totalprice" />
        <property name="o_orderdate" column="o_orderdate" />
        <property name="o_orderpriority" column="o_orderpriority" />
        <property name="o_orderstatus" column="o_orderstatus" />
        <property name="o_clerk" column="o_clerk" />
        <property name="o_shippriority" column="o_shippriority" />
        <property name="o_comment" column="o_comment" />
    </class>
</hibernate-mapping>