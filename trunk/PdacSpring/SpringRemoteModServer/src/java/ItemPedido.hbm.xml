        <composite-id name="pk" class="br.uniriotec.np2tec.propid.shared.classes.ItemPedidoPK" > 
            <key-property name="L_ORDERKEY" column="L_ORDERKEY" />
            <key-property name="L_LINENUMBER" column="L_LINENUMBER" /> 
        </composite-id> 
        
        <property name="L_PARTKEY" column="L_PARTKEY" />
        <property name="L_SUPPKEY" column="L_SUPPKEY" />
        <property name="L_QUANTITY" column="L_QUANTITY" />
        
        <property name="L_EXTENDEDPRICE" column="L_EXTENDEDPRICE" />
        <property name="L_DISCOUNT" column="L_DISCOUNT" />
        <property name="L_TAX" column="L_TAX" />
        
        <property name="L_RETURNFLAG" column="L_RETURNFLAG" />
        <property name="L_LINESTATUS" column="L_LINESTATUS" />
        <property name="L_SHIPDATE" column="L_SHIPDATE" />
        <property name="L_COMMITDATE" column="L_COMMITDATE" />
        <property name="L_RECEIPTDATE" column="L_RECEIPTDATE" />
        <property name="L_SHIPINSTRUCT" column="L_SHIPINSTRUCT" />
        <property name="L_SHIPMODE" column="L_SHIPMODE;" />
        <property name="L_COMMENT" column="L_COMMENT" />
    </class>
</hibernate-mapping>